{
    "name": "helicon/type-converter",
    "description": "Simple typecasting system for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "polidog",
            "email": "polidogs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Helicon\\TypeConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helicon\\TypeConverter\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/simple-cache": "^1.0",
        "laminas/laminas-hydrator": "^4.1",
        "helicon/object-type-parser": "^1.0",
        "nesbot/carbon": "^2.26"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "scripts": {
        "cs-fix": ["php-cs-fixer fix -v"],
        "phpunit": "./vendor/bin/phpunit tests/"
    }
}
